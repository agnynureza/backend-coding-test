{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 6
    },
    "rules": {
        "indent":[
            "error",
            4
        ],
        "semi": [
            "error",
            "always"
        ],
        "max-len": [
            1,
            150,
            200
        ]
    }
}
