{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true,
        "mocha": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 6
    },
    "rules": {
        "indent":[
            "error",
            4
        ],
        "semi": [
            "error",
            "always"
        ],
        "max-len": "off",
        "consistent-return":"off",
        "no-shadow":"off"
    }
}
